{
  "name": "@feedma/http-resource",
  "version": "0.0.1",
  "description": "A class based HTTP client built on top of axios",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feedma/ts-http-resource.git"
  },
  "keywords": [
    "HTTPClient",
    "Axios",
    "Resource",
    "HTTP"
  ],
  "files": [
    "lib/**/*"
  ],
  "author": {
    "name": "Feedma"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feedma/ts-http-resource/issues"
  },
  "homepage": "https://github.com/feedma/ts-http-resource#readme",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "axios": "^0.27.2",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "jest-mock-axios": "^4.6.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "axios": "^0.27.2"
  }
}
