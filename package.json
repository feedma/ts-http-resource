{
  "name": "@feedma/http-service",
  "private": false,
  "version": "0.0.0-development",
  "description": "A class based HTTP client built on top of axios",
  "main": "dist/index.js",
  "module": "dist/ts-http-service.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:cov": "tsdx test --coverage",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/feedma/ts-http-service.git"
  },
  "keywords": [
    "HTTPClient",
    "Axios",
    "Service",
    "REST",
    "HTTP"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": {
    "name": "Feedma"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feedma/ts-http-service/issues"
  },
  "homepage": "https://feedma.github.io/ts-http-service",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/jest": "^29.1.0",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "axios": "^0.27.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.1.1",
    "jest-mock-axios": "^4.6.1",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vuepress": "^2.0.0-beta.49"
  },
  "peerDependencies": {
    "axios": "^0.27.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "size-limit": [
    {
      "path": "dist/ts-http-service.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ts-http-service.esm.js",
      "limit": "10 KB"
    }
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
